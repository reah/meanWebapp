{
    "name": "Drive Button",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Open Google Drive",
    "browser_action": {
        "default_icon": "chrome_ext/icon.png"
    },
    "background": {
        "scripts": [
            "chrome_ext/background.js"
        ]
    },
    "permissions": [
    "http://localhost:3000/*",
    "http://*/",
    "tabs",
    "contextMenus"
    ]
}